{
	"license": "MIT",
	"name": "necord",
	"version": "2.0.0",
	"description": "A Discord bot framework built on top of Discord.js and Nest.js for advanced and amazing bots.",
	"keywords": [
		"nest",
		"nestjs",
		"nestjs-module",
		"discord",
		"discordjs",
		"discord bot",
		"discord bot api",
		"bot",
		"bot api",
		"bot framework"
	],
	"homepage": "https://github.com/SocketSomeone/necord/wiki",
	"repository": {
		"type": "git",
		"url": "https://github.com/SocketSomeone/necord"
	},
	"bugs": {
		"url": "https://github.com/SocketSomeone/necord/issues"
	},
	"author": "Alexey Filippov <socket.someone@gmail.com>",
	"scripts": {
		"build": "rimraf -rf dist && tsc -p tsconfig.json",
		"precommit": "lint-staged",
		"prepublish:npm": "npm run build",
		"publish:npm": "npm publish --access public",
		"prepublish:dev": "npm run build",
		"publish:dev": "npm publish --access public --tag dev",
		"prepare": "husky install .github/husky",
		"test": "",
		"lint": "eslint --ignore-path .gitignore {integration,src}/**/*.ts",
		"release": "release-it"
	},
	"lint-staged": {
		"*.ts": [
			"prettier --write"
		]
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"devDependencies": {
		"@commitlint/cli": "13.2.1",
		"@commitlint/config-angular": "13.2.0",
		"@favware/npm-deprecate": "^1.0.4",
		"@nestjs/common": "^8.0.0",
		"@nestjs/core": "^8.0.0",
		"@types/node": "16.11.6",
		"@typescript-eslint/eslint-plugin": "4.33.0",
		"@typescript-eslint/parser": "4.33.0",
		"discord.js": "13.3.1",
		"eslint": "7.32.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-prettier": "4.0.0",
		"husky": "7.0.4",
		"lint-staged": "11.2.6",
		"prettier": "2.4.1",
		"reflect-metadata": "0.1.13",
		"release-it": "^14.11.8",
		"rimraf": "3.0.2",
		"rxjs": "7.4.0",
		"typescript": "4.4.4"
	},
	"peerDependencies": {
		"@nestjs/common": "^8.0.0",
		"@nestjs/core": "^8.0.0",
		"discord.js": "^13.3.1",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.2.0"
	},
	"peerDependenciesMeta": {
		"class-validator": {
			"optional": true
		},
		"class-transformer": {
			"optional": true
		}
	},
	"engines": {
		"node": ">=16.6.0"
	},
	"dependencies": {}
}
